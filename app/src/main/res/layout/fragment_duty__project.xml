<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/line_duty"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="com.kongtiaoapp.xxhj.duty.fragment.Duty_ProjectFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/project_name_height"
        android:layout_marginLeft="20dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_project_duty_person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/project_duty_person"
            android:textColor="@color/a999999"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/txt_project_duty_person_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:textColor="@color/a333333"
            android:textSize="@dimen/project_runningsize" />
    </LinearLayout>

    <include layout="@layout/include_line_project"></include>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/project_name_height"
        android:layout_marginLeft="20dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txt_project_connect_tel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/project_connect_tel"
            android:textColor="@color/a999999"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/txt_project_connect_tel_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:textColor="@color/a333333"
            android:textSize="@dimen/project_runningsize" />
    </LinearLayout>

    <include layout="@layout/include_line_project"></include>

