<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/frame_freeze_temp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/paint_height">

        <RelativeLayout
            android:id="@+id/rela_paint8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/calendar_img"
            android:paddingBottom="20dp"
            android:background="@color/ffffff"></RelativeLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/loading_mrgt_fator"
            android:layout_marginTop="@dimen/tuli_margintp"
            android:orientation="vertical"
            android:visibility="visible">

            <TextView
                android:id="@+id/graph_freezeT1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/loading_one"
                android:textSize="@dimen/loading_factor"
                android:visibility="gone" />

            <TextView
                android:id="@+id/graph_freezeT2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/loading_two"
                android:textSize="@dimen/loading_factor"
                android:visibility="gone" />

            <TextView
                android:id="@+id/graph_freezeT3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/loading_three"
                android:textSize="@dimen/loading_factor"
                android:visibility="gone" />

            <TextView
                android:id="@+id/graph_freezeT4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/loading_four"
                android:textSize="@dimen/loading_factor"
                android:visibility="gone" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/loading_mtop_factor"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txt_notata8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/no_data"
                android:textColor="@color/red"
                android:textSize="@dimen/font_big"
                android:visibility="gone" />
        </LinearLayout>


    </FrameLayout>

</LinearLayout>