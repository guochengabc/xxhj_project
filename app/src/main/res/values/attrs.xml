<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- PagerSlidingTab -->
    <declare-styleable name="PagerSlidingTab">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="pst_dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabBackgrounds" format="reference" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="pst_textAllCaps" format="boolean" />
    </declare-styleable>
    <!--九宫格GridView-->
    <declare-styleable name="NineGridView">
        <attr name="gap" format="dimension" />
    </declare-styleable>
    <!-- roundedimageview -->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    <!--自定义上下拉刷新-->
    <declare-styleable name="SpringView">
        <attr name="type">
            <enum name="overlap" value="0" />
            <enum name="follow" value="1" />
        </attr>
        <attr name="give">
            <enum name="both" value="0" />
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
            <enum name="none" value="3" />
        </attr>
        <attr name="header" format="reference" />
        <attr name="footer" format="reference" />
    </declare-styleable>
    <!--图片剪切-->
    <declare-styleable name="CropImageView">
        <attr name="cropMaskColor" format="color" />
        <attr name="cropBorderColor" format="color" />
        <attr name="cropBorderWidth" format="dimension" />
        <attr name="cropFocusWidth" format="dimension" />
        <attr name="cropFocusHeight" format="dimension" />
        <attr name="cropStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>
<!--时间选择器-->
    <declare-styleable name="PickerView">
        <attr name="isLoop" format="boolean" />
    </declare-styleable>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="textSize" format="dimension"/>
        <attr name="textColorOut" format="color"/>
        <attr name="textColorCenter" format="color"/>
        <attr name="dividerColors" format="color"/>
    </declare-styleable>
    <declare-styleable name="ProgressBar_Report_Data">
        <attr name="textColor" format="color" />
    </declare-styleable>
</resources>